  <label for="team_name">Team Name:</label>
  <input id="team_name" type="text" name="team[name]"><br>
  <label for="team_motto">Team Motto:</label>
  <input id="team_motto" type="text" name="team[motto]"><br>

  <h2>Hero 1</h2>
  <label for="member1_name">Hero Name:</label>
  <input id="member1_name" type="text" name="team[members][][name]">

  <label for="member1_power">Hero Power:</label>
  <input id="member1_power" type="text" name=team[members][][power]>

  <label for="member1_bio">Hero Biography:</label>
  <input id="member1_bio" type="text" name=team[members][][bio]><br>

  <h2>Hero 2</h2>
  <label for="member2_name">Hero Name:</label>
  <input id="member2_name" type="text" name=team[members][][name]>

  <label for="member2_power">Hero Power:</label>
  <input id="member2_power" type="text" name=team[members][][power]>

  <label for="member2_bio">Hero Biography:</label>
  <input id="member2_bio" type="text" name=team[members][][bio]><br>

  <h2>Hero 3</h2>
  <label for="member3_name">Hero Name:</label>
  <input id="member3_name" type="text" name=team[members][][name]>

  <label for="member3_power">Hero Power:</label>
  <input id="member3_power" type="text" name=team[members][][power]>

  <label for="member3_bio">Hero Biography:</label>
  <input id="member3_bio" type="text" name=team[members][][bio]><br>


  <input type="submit" name="submit" value="submit">
</form>

